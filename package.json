{
  "name": "@hackplan/thinker-js",
  "version": "1.1.1",
  "description": "Another synchronization framework",
  "main": "dist/main.js",
  "jsnext:main": "./lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --no-cache",
    "opendoc": "gulp doc && open docs/classes.list.html",
    "start": "gulp",
    "build": "webpack --progress"
  },
  "keywords": [
    "synchronization",
    "framework"
  ],
  "author": "c4605 <bolasblack@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash-es": "^4.17.10",
    "wolfy87-eventemitter": "5.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./jest-test-framework-setup.js",
    "testRegex": "(/__tests__/[^_].*|\\.(test|spec))\\.(js|coffee)$",
    "roots": [
      "<rootDir>/lib"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee"
    ]
  },
  "devDependencies": {
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-strict-mode": "^6.11.3",
    "babel-preset-env": "^1.7.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "1.10.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.1",
    "gulp-coffee": "2.3.2",
    "gulp-jsdoc3": "^1.0.0",
    "jest": "^23.4.1",
    "lodash": "4.15.0",
    "lolex": "1.5.1",
    "bluebird": "3.4.6",
    "webpack": "^4.16.1",
    "webpack-command": "^0.4.1"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.10"
  }
}
